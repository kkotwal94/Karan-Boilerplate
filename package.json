    "docs:build": "documentation build 'server/**' -f html -o docs"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "jest --bail --findRelatedTests",
      "prettier --write",
      "git add"
    ],
    "*.scss": [
      "stylelint --fix",
      "prettier --write",
      "git add"
    ]
