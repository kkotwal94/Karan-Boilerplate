  "scripts": {
    "babel-node": "babel-node",
    "lint:js": "eslint . --fix",
    "format": "prettier --write '**/*.{js,css,md}'",
    "build": "babel server/ -d lib/",
    "prepublish": "npm run build",
    "start": "babel-node server/server.js",
